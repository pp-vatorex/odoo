<odoo>

<template id="assets_frontend" inherit_id="website.assets_frontend" name="HTML snippet frontend assets">
    <xpath expr="." position="inside">
        <link rel="stylesheet"
              href="/vatorex_custom_blocks/static/src/scss/website_snippet_html.scss"/>
    </xpath>
</template>

<template id="assets_editor" inherit_id="website.assets_editor">
    <xpath expr="script[last()]" position="after">
        <script type="text/javascript"
                src="/vatorex_custom_blocks/static/src/js/lz-string.min.js"/>     
        <script type="text/javascript"
                src="/vatorex_custom_blocks/static/src/js/options.js"/>
    </xpath>
</template>

</odoo>
